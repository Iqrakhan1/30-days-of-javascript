
// Activity 4: Logical oprators

// Task 11 use && oprator 


let year = 2004;

if(year%4 == 0 && year %100 != 0){
    console.log("Leap year");
}
else{
    console.log(" Not leap year")
}


//Task 12 


let a = 4;
let b= 10;

if(a>0 || b>0){
    console.log("Either a or b  is greater then 0");
}


