//Activity 1  Function declaration

//Task 1 write a function to check number is even or odd

function evenodd(num){
    if(num%2==0){
        console.log(num + " is Even");
    }
    else{
        console.log(num + "is Odd");
    }
}
evenodd(5);
evenodd(10);

//Task 2 clacualte squre of number 

function squr(num1){

console.log(num1 * num1);
}
squr(5);


