//Task 3 maximum of two numbwe  using fuction expressions

//maximum of two numbwe using fuction expressions

function maximum(num1, num2) {
    if (num1 > num2) {
      return num1;
    } else {
      return num2;
    }
  }
  console.log("maximum of two number is " + maximum(45, 78));


//task 4 concatinate two string and return result

function concatination(str1, str2) {
  return str1.concat(str2);
}
console.log(concatination("hello " , "Muskan!")); 

